{
  // https://unpkg.com/@tsconfig/svelte/tsconfig.json
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended",
  "_version": "2.0.0",

  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
