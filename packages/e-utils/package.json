{
  "name": "@elinzy/e-utils",
  "version": "0.4.13",
  "description": "e Tools, a collection of common JavaScript / TypeScript utilities.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "moduleName": "eUtils",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "rollup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsx scripts/build.ts",
    "build:rollup": "rollup -c rollup.config.*",
    "build:types": "tsc --noEmit",
    "publish": "tsx scripts/publish.ts",
    "release": "tsx scripts/release.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "*.d.ts",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appleec/e-utils.git"
  },
  "keywords": [
    "e",
    "utils",
    "e-utils",
    "elinzy",
    "elinzy/e-utils"
  ],
  "author": "Appleec <771992522@qq.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/Appleec/e-utils/issues"
  },
  "homepage": "https://github.com/Appleec/e-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.12",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.1",
    "@vitest/coverage-istanbul": "^3.0.5",
    "ansis": "^3.16.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.23.1",
    "execa": "^9.5.2",
    "jsdom": "^25.0.0",
    "ofetch": "^1.4.1",
    "prompts": "^2.4.2",
    "rollup": "^4.21.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "semver": "^7.7.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.4"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
