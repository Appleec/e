{
  "name": "@elinzy/eslint-config",
  "version": "0.1.1",
  "description": "Common ESLint configuration for e",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup --watch",
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c eslint.config.mjs ."
  },
  "exports": {
    ".": {
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./flat/recommended": {
      "module": "./dist/configs/flat/recommended.cjs",
      "import": "./dist/configs/flat/recommended.mjs",
      "require": "./dist/configs/flat/recommended.cjs"
    },
    "./eslintrc/base/recommended": {
      "module": "./dist/configs/eslintrc/base/recommended.js",
      "import": "./dist/configs/eslintrc/base/recommended.js",
      "require": "./dist/configs/eslintrc/base/recommended.js"
    },
    "./eslintrc/base/recommended-script": {
      "module": "./dist/configs/eslintrc/base/recommended-script.js",
      "import": "./dist/configs/eslintrc/base/recommended-script.js",
      "require": "./dist/configs/eslintrc/base/recommended-script.js"
    },
    "./eslintrc/base/recommended-module": {
      "module": "./dist/configs/eslintrc/base/recommended-module.js",
      "import": "./dist/configs/eslintrc/base/recommended-module.js",
      "require": "./dist/configs/eslintrc/base/recommended-module.js"
    },
    "./eslintrc/base/typescript": {
      "module": "./dist/configs/eslintrc/base/typescript.js",
      "import": "./dist/configs/eslintrc/base/typescript.js",
      "require": "./dist/configs/eslintrc/base/typescript.js"
    },
    "./eslintrc/node/recommended": {
      "module": "./dist/configs/eslintrc/node/recommended-script.js",
      "import": "./dist/configs/eslintrc/node/recommended-script.js",
      "require": "./dist/configs/eslintrc/node/recommended-script.js"
    },
    "./eslintrc/node/recommended-script": {
      "module": "./dist/configs/eslintrc/node/recommended-script.js",
      "import": "./dist/configs/eslintrc/node/recommended-script.js",
      "require": "./dist/configs/eslintrc/node/recommended-script.js"
    },
    "./eslintrc/node/recommended-module": {
      "module": "./dist/configs/eslintrc/node/recommended-module.js",
      "import": "./dist/configs/eslintrc/node/recommended-module.js",
      "require": "./dist/configs/eslintrc/node/recommended-module.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appleec/e.git",
    "directory": "packages/e-eslint-config"
  },
  "keywords": [
    "e",
    "eslint",
    "config",
    "eslint-config"
  ],
  "author": {
    "name": "Appleec",
    "email": "771992522@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Appleec/e/issues"
  },
  "homepage": "https://github.com/Appleec/e#readme",
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.27.1",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "mlly": "1.7.4",
    "rollup": "4.41.1",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-node-externals": "8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "eslint": "^9.22.0",
    "eslint-import-resolver-next": "0.6.0",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-import-x": "4.15.2",
    "eslint-plugin-n": "17.16.2",
    "eslint-plugin-promise": "7.2.1",
    "globals": "^16.0.0",
    "local-pkg": "1.1.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.26.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-import-x": "4.x",
    "eslint-plugin-n": "17.x",
    "typescript": ">=4.8.4 <5.9.0",
    "typescript-eslint": "8.x"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-plugin-n": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  }
}
